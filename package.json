{
    "name": "react-scaffold",
    "version": "1.0.0",
    "description": "一个简单的 React 脚手架",
    "scripts": {
        "dev": "node ./build/server.js",
        "build": "rm -rf ./dist && npm run eslint && webpack --config ./build/webpack.prod.config.js",
        "eslint": "npm cache clean && eslint --ext .jsx .js src",
        "format": "prettier --single-quote true --semi false --trailing-comma none --write ./src/*/*/*.js",
        "precommit": "lint-staged"
    },
    "author": "i@starriv.com",
    "license": "MIT",
    "dependencies": {
        "autoprefixer-loader": "^3.2.0",
        "babel-core": "^6.24.1",
        "babel-loader": "^6.4.1",
        "babel-polyfill": "^6.23.0",
        "babel-preset-env": "^1.4.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "chunk-manifest-webpack-plugin": "^1.0.0",
        "css-loader": "^0.27.3",
        "extract-text-webpack-plugin": "^2.1.0",
        "file-loader": "^0.9.0",
        "html-webpack-plugin": "^2.28.0",
        "less": "^2.7.2",
        "less-loader": "^4.0.3",
        "postcss-loader": "^0.9.1",
        "pre-commit": "^1.1.3",
        "prop-types": "^15.5.8",
        "react": "^15.5.4",
        "react-dom": "^15.5.4",
        "react-hot-loader": "^1.3.1",
        "react-redux": "^4.4.8",
        "react-router-dom": "^4.1.1",
        "react-router-redux": "^5.0.0-alpha.6",
        "redux": "^3.6.0",
        "redux-devtools": "^3.3.2",
        "redux-devtools-dock-monitor": "^1.1.1",
        "redux-devtools-log-monitor": "^1.2.0",
        "redux-form": "^5.3.5",
        "redux-logger": "^2.10.2",
        "redux-thunk": "^2.2.0",
        "style-loader": "^0.16.0",
        "url-loader": "^0.5.7",
        "webpack": "^2.4.1",
        "webpack-chunk-hash": "^0.4.0",
        "webpack-dev-server": "^2.4.2",
        "webpack-merge": "^4.1.0"
    },
    "pre-commit": [
        "eslint"
    ],
    "lint-staged": {
        "*.js": [
            "prettier --write"
        ]
    },
    "devDependencies": {
        "babel-eslint": "^7.2.2",
        "eslint": "^3.5.0",
        "eslint-config-standard": "^10.2.1",
        "eslint-config-standard-react": "^4.3.0",
        "eslint-loader": "^1.5.0",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-node": "^4.2.2",
        "eslint-plugin-promise": "^3.5.0",
        "eslint-plugin-react": "^6.2.1",
        "eslint-plugin-standard": "^3.0.1",
        "husky": "^0.13.3",
        "lint-staged": "^3.4.0"
    }
}
